<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dwiktorowski.catalogofnotes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon_v1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon_v1_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".formula_insert" />
        <activity
            android:name=".NoteInsert"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name=".NoteWithFormulaInsert"></activity>
    </application>

</manifest>